{
  "name": "@xenopus/final-form",
  "version": "0.0.1",
  "main": "index.js",
  "license": "MIT",
  "typings": "build/module/index.d.ts",
  "module": "build/module/index.js",
  "scripts": {
    "build:declaration": "tsc --project tsconfig.json",
    "build": "BABEL_ENV=build babel src --root-mode upward --out-dir build/module --source-maps --extensions .ts,.tsx --delete-dir-on-start",
    "build:watch": "yarn build --watch",
    "dev": "yarn build:watch",
    "clean": "rm -rf ./build && rm -rf tsconfig.tsbuildinfo"
  },
  "peerDependencies": {
    "@xenopus/ui": "^0.0.1",
    "@xenopus/viewmodel": "^0.0.1"
  },
  "dependencies": {
    "antd": "4.0.0-alpha.9",
    "final-form": "^4.18.6",
    "lodash": "^4.17.15",
    "react-final-form": "^6.3.0"
  },
  "devDependencies": {
    "typescript": "^3.7.2"
  }
}
